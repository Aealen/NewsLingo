{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "dev环境",
      "type": "dart",
      "args": [
        "--dart-define=INIT_ENV=dev",
        "--dart-define=ANDROID_CHANNEL=flutter"
      ],
      "request": "launch",
      "program": "lib/main.dart"
    },
    {
      "name": "test环境",
      "type": "dart",
      "args": [
        "--dart-define=INIT_ENV=test",
        "--dart-define=ANDROID_CHANNEL=flutter"
      ],
      "request": "launch",
      "program": "lib/main.dart"
    },
    {
      "name": "pre环境",
      "type": "dart",
      "args": [
        "--dart-define=INIT_ENV=pre",
        "--dart-define=ANDROID_CHANNEL=flutter"
      ],
      "request": "launch",
      "program": "lib/main.dart"
    },
    {
      "name": "prod环境",
      "type": "dart",
      "args": [
        "--dart-define=INIT_ENV=prod",
        "--dart-define=ANDROID_CHANNEL=flutter"
      ],
      "request": "launch",
      "program": "lib/main.dart"
    },
    {
      "name": "Web: dev环境 (绕过CORS)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d chrome",
        "--dart-define=INIT_ENV=dev",
        "--web-flags=--disable-web-security,--disable-features=BlockInsecurePrivateNetworkRequests"
      ]
    },
    {
      "name": "Web: test环境 (绕过CORS)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d chrome",
        "--dart-define=INIT_ENV=test",
        "--web-flags=--disable-web-security,--disable-features=BlockInsecurePrivateNetworkRequests"
      ]
    },
    {
      "name": "Web: pre环境 (绕过CORS)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d chrome",
        "--dart-define=INIT_ENV=pre",
        "--web-flags=--disable-web-security,--disable-features=BlockInsecurePrivateNetworkRequests"
      ]
    },
    {
      "name": "Web: prod环境 (绕过CORS)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d chrome",
        "--dart-define=INIT_ENV=prod",
        "--web-flags=--disable-web-security,--disable-features=BlockInsecurePrivateNetworkRequests"
      ]
    }
  ]
}
